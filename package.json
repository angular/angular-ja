    "execa": "^9.3.0",
    "globby": "14.0.2",
    "langchain": "0.3.28",
    "p-limit": "^7.2.0",
    "prh": "5.4.4",
    "rxjs": "7.8.1",
    "sitemap": "8.0.0",
    "textlint": "^14.0.4",
    "textlint-filter-rule-allowlist": "4.0.0",
