  "author": "Suguru Inatomi <suguru.inatomi@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "npm run textlint",
    "update-origin": "./update-origin.sh",
    "textlint": "textlint 'aio-ja/**/!(*.en).md'",
